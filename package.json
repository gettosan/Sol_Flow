  "author": "gettosan <gettoojjk@gmail.com>",
  "license": "ISC",
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/gettosan/Sol_Flow/issues"
  },
  "homepage": "https://github.com/gettosan/Sol_Flow#readme",
